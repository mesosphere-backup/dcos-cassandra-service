{
  "assets": {
    "uris": {
      "scheduler-zip":"{{artifact-dir}}/scheduler.zip",
      "executor-zip":"{{artifact-dir}}/executor.zip",
      "jre-tar-gz":"https://s3-us-west-2.amazonaws.com/dse-development/server-jre-8u74-linux-x64.tar.gz",
      "apache-cassandra-bin-tar-gz":"https://s3-us-west-2.amazonaws.com/dse-development/dse-5.0.0-bin.tar.gz"
    }
  },
  "images": {
    "icon-small": "https://downloads.mesosphere.com/datastax/assets/48x48.png",
    "icon-medium": "https://downloads.mesosphere.com/datastax/assets/96x96.png",
    "icon-large": "https://downloads.mesosphere.com/datastax/assets/256x256.png"
  },
  "cli":{
    "binaries":{
      "darwin":{
        "x86-64":{
          "contentHash":[
            {
              "algo":"sha256",
              "value":"{{cli-darwin-sha256q}}"
            }
          ],
          "kind":"executable",
          "url":"{{artifact-dir}}/cli/darwin/dcos-cassandra"
        }
      },
      "linux":{
        "x86-64":{
          "contentHash":[
            {
              "algo":"sha256",
              "value":"{{cli-linux-sha256}}"
            }
          ],
          "kind":"executable",
          "url":"{{artifact-dir}}/cli/linux/dcos-cassandra"
        }
      }
    }
  }
}
